# 数据库配置 - 不需要真正连接
DATABASE_URL="mysql://root:datascope@localhost:3306/datascope"

# 服务器配置
PORT=5000
NODE_ENV=development
LOG_LEVEL=debug

# 安全配置
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=24h
ENCRYPTION_KEY=your_encryption_key_32chars_long

# API配置
API_PREFIX=/api/v1
CORS_ORIGIN=*

# 启用模拟数据模式 - 最重要的设置
USE_MOCK_DATA=true 