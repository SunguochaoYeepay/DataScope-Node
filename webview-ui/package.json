{
  "name": "datascope-ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "predev": "node scripts/check-port.js && ./scripts/verify-datasource-service.sh || (echo '尝试自动修复数据源服务文件...' && ./scripts/restore-datasource-service.sh)",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts",
    "dev:network": "vite --host 0.0.0.0",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "migrate-passwords": "ts-node src/scripts/migrate-passwords.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "init:db": "ts-node scripts/init-db.ts",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:query-plan": "node test/query-plan.test.js",
    "verify:datasource": "./scripts/verify-datasource-service.sh",
    "fix:datasource": "./scripts/restore-datasource-service.sh",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.7",
    "echarts": "^5.6.0",
    "element-plus": "^2.6.2",
    "pinia": "^2.1.7",
    "uuid": "^11.1.0",
    "vue": "^3.4.21",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.3.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@types/axios": "^0.9.36",
    "@types/node-fetch": "^2.6.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.23.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^2.2.19",
    "typescript": "^5.2.2",
    "vite": "^6.2.3",
    "vitest": "^3.0.9",
    "vue-tsc": "^1.8.27"
  }
}
