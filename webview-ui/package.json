{
  "name": "datascope-ui",
  "private": true,
  "version": "0.0.1",
  "description": "数据查询与可视化平台",
  "type": "module",
  "scripts": {
    "dev": "node scripts/kill-port.cjs 8080 && cross-env VITE_USE_MOCK_API=true vite --port 8080 --strictPort --clearScreen=false",
    "dev:fixed": "node scripts/kill-port.cjs 8080 && cross-env VITE_USE_MOCK_API=true vite --port 8080 --strictPort --clearScreen=false",
    "dev:network": "node scripts/kill-port.cjs 8080 && cross-env VITE_USE_MOCK_API=true vite --host 0.0.0.0 --port 8080 --strictPort --clearScreen=false",
    "dev:no-hmr": "node scripts/kill-port.cjs 8080 && cross-env VITE_DISABLE_HMR=true cross-env VITE_USE_MOCK_API=true vite --port 8080 --strictPort --clearScreen=false",
    "dev:stable": "node scripts/kill-port.cjs 8080 && cross-env VITE_USE_MOCK_API=true vite --port 8080 --strictPort --clearScreen=false --debug --no-sourcemap",
    "dev:final": "npm run clean:all && node scripts/kill-port.cjs 8080 && cross-env NODE_ENV=development cross-env VITE_USE_MOCK_API=true vite --port 8080 --config vite.dev.config.js",
    "dev:simple": "npm run clean:all && node scripts/kill-port.cjs 8080 && cross-env VITE_USE_MOCK_API=true vite --config vite.simple.js --port 8080",
    "dev:preview": "npm run clean:all && cross-env VITE_USE_MOCK_API=true vite build && node scripts/kill-port.cjs 8080 && cross-env VITE_USE_MOCK_API=true vite preview --port 8080",
    "build:stable": "cross-env VITE_USE_MOCK_API=false NODE_ENV=production vite build --debug",
    "preview:stable": "npm run clean:all && npm run build:stable && node scripts/kill-port.cjs 8080 && vite preview --port 8080",
    "clean:ports": "node scripts/kill-port.cjs 8080",
    "clean:cache": "rm -rf node_modules/.vite* node_modules/.vite_* .vite* dist tmp .temp",
    "clean:all": "npm run clean:ports && npm run clean:cache",
    "dev:clean": "npm run clean:all && npm run dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "preview:dev": "node scripts/kill-port.cjs 8080 && vite preview --port 8080 --strictPort",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:query-plan": "node test/query-plan.test.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@vueuse/core": "^10.9.0",
    "ant-design-vue": "^4.0.3",
    "axios": "^1.6.7",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "element-plus": "^2.6.2",
    "naive-ui": "^2.41.0",
    "pinia": "^2.1.7",
    "terser": "^5.39.0",
    "uuid": "^11.1.0",
    "vue": "^3.4.21",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.3.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@types/axios": "^0.9.36",
    "@types/node-fetch": "^2.6.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-vue": "^4.6.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.23.0",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^2.2.19",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vitest": "^3.0.9",
    "vue-tsc": "^1.8.27"
  }
}
